{
  "name": "@contactlab/appy",
  "version": "4.0.1",
  "description": "A functional wrapper around Fetch API",
  "main": "./index.js",
  "module": "./_es6/index.js",
  "typings": "./index.d.ts",
  "sideEffects": false,
  "author": "Contactlab",
  "license": "Apache-2.0",
  "homepage": "https://contactlab.github.io/appy",
  "bugs": "https://github.com/contactlab/appy/issues",
  "repository": "contactlab/appy",
  "keywords": [
    "contactlab",
    "appy",
    "fetch",
    "fp",
    "fp-ts",
    "typescript"
  ],
  "engines": {
    "node": ">= 14.0",
    "npm": ">= 6.0"
  },
  "scripts": {
    "postinstall": "husky install",
    "check": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pretest": "npm run lint && npm run check",
    "test": "jest",
    "prebuild": "rm -rf ./dist",
    "build": "tsc -p ./tsconfig.build.json && tsc -p ./tsconfig.build-es6.json",
    "postbuild": "ts-node scripts/prepare-dist",
    "prepublishOnly": "ts-node scripts/pre-publish",
    "prerelease": "npm test && npm run build",
    "release": "ts-node scripts/release",
    "docs": "docs-ts",
    "docs:update": "npm run docs && git add docs",
    "postdocs": "ts-node scripts/docs"
  },
  "peerDependencies": {
    "fp-ts": "^2.8.0"
  },
  "devDependencies": {
    "@types/fetch-mock": "^7.3.3",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.18",
    "@types/node": "^14.14.25",
    "abort-controller": "^3.0.0",
    "cross-fetch": "^3.0.6",
    "docs-ts": "^0.6.4",
    "eslint": "^7.15.0",
    "eslint-config-contactlab": "^6.0.0",
    "eslint-config-prettier": "^8.0.0",
    "fetch-mock": "^9.11.0",
    "fp-ts": "^2.8.0",
    "glob": "^7.1.6",
    "husky": "^6.0.0",
    "io-ts": "^2.2.13",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.2"
  }
}
